{
  "name": "outlook-calendar-sync",
  "version": "1.0.0",
  "description": "Sync outlook calendar",
  "main": "server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev:server": "nodemon --inspect=0.0.0.0:9228 --exec babel-node -w server server/index.js",
    "dev:client": "webpack-dev-server --inline --progress --config client/build/webpack.dev.conf.js",
    "dev": "npm run dev:server && npm run dev:client",
    "lint": "eslint --ext .js,.vue client/src,server",
    "lint:fix": "eslint --fix . --ext .js,.vue ./client/src,./server",
    "build:client": "node client/build/build.js",
    "build:server": "babel ./server --out-dir dist/server --copy-files",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "start": "NODE_ENV=production node dist/server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
